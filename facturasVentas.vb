SELECT DISTINCT
    factura.NOMBREDESTINATARIO AS Cliente
FROM 
    V_TRFACTURAVENTA_ factura
WHERE 
    factura.BO_PLACE_ID = '{FBC69AAF-7CEE-4D9D-8C49-F50893BD6E0C}'
    AND factura.TIPOTRANSACCION_ID = '{51DFC3EB-13BB-4CF4-82A6-2CA7CEEE524A}'
    AND factura.UNIDADOPERATIVA_ID = '{B7DF50E0-4EDA-4DF0-81E5-CA70A3A5EFCD}';